@model MvcMember.Models.V_Consume

@{
    ViewBag.Title = "消费积分详细";
}

<h2>消费积分详细</h2>

<fieldset>
    <legend>@Model.Name 的消费积分详情</legend>
    <table id="mbtable" style="border: 0;">
        <tr>
            <td>会员姓名</td>
            <td>@Model.Name</td>
        </tr>
        <tr>
            <td>联系电话</td>
            <td>@Model.Mobile</td>
        </tr>
        <tr>
            <td>车牌号码</td>
            <td>@Model.CarNO</td>
        </tr>
        <tr>
            <td>会员卡号</td>
            <td>@Model.CardNo</td>
        </tr>
        <tr>
            <td>积分单号</td>
            <td>@Model.BillNo</td>
        </tr>
        <tr>
            <td>消费时间</td>
            <td>@Model.CreateDate</td>
        </tr>
        <tr>
            <td>期初剩余积分</td>
            <td>@Model.CurBIntegral</td>
        </tr>
        <tr>
            <td>期初余额</td>
            <td>@Model.BalanceMoney</td>
        </tr>
        <tr>
            <td>合计应收金额</td>
            <td>@Model.SumFee</td>
        </tr>
        <tr>
            <td>本次积分</td>
            <td>@Model.Integral</td>
        </tr>
        <tr>
            <td>本次用积分抵用金额</td>
            <td>@Model.InteToMoney</td>
        </tr>
        <tr>
            <td>本次抵用使用积分</td>
            <td>@Model.DeductInte</td>
        </tr>
        <tr>
            <td>备注</td>
            <td>@Model.Remark</td>
        </tr>
        <tr>
            <td>操作员</td>
            <td>@Model.Creator</td>
        </tr>
        <tr>
            <td>操作时间</td>
            <td>@Model.CreateDate</td>
        </tr>
    </table>
    <table id="jslist" class="table70">
        <tr>
            <th>业务项目</th>
            <th>工时费</th>
            <th>配件费</th>
            <th>其他费</th>
            <th>小计</th>
            <th>抵用结算</th>
            <th>使用积分</th>
            <th>应收金额</th>
            <th>本次积分</th>
        </tr>
        @foreach (var st in ViewBag.ServiceType)
        {
            <tr>
                <td>@st.ServiceType</td>
                <td>@st.HoursFee</td>
                <td>@st.PartsFee</td>
                <td>@st.OtherFee</td>
                <td>@st.SumFee</td>
                <td>@st.ToFee</td>
                <td>@st.UseInte</td>
                <td>@st.FactFee</td>
                <td>@st.Integral</td>
            </tr>
        }
    </table>
    <table id="sklist" class="table40">
        <tr>
            <th>支付方式</th>
            <th>支付金额</th>
            <th>支付时间</th>
        </tr>
        @foreach (var st in ViewBag.PayList)
        {
            <tr>
                <td>@st.PayWay</td>
                <td>@st.PayMoney</td>
                <td>@st.PayTime</td>
            </tr>
        }
    </table>
</fieldset>
<p style="padding-left: 5px;">
    @Html.ActionLink("返回会员信息列表", "Index")
</p>
